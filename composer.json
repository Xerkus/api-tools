{
    "name": "laminas-api-tools/api-tools",
    "description": "Laminas API Tools module for Laminas",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "framework"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools/issues",
        "source": "https://github.com/laminas-api-tools/api-tools",
        "rss": "https://github.com/laminas-api-tools/api-tools/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0",
        "laminas-api-tools/api-tools-content-validation": "~1.0",
        "laminas-api-tools/api-tools-hal": "~1.0",
        "laminas-api-tools/api-tools-mvc-auth": "~1.0",
        "laminas-api-tools/api-tools-oauth2": "~1.0",
        "laminas-api-tools/api-tools-provider": "~1.0",
        "laminas-api-tools/api-tools-rest": "~1.0",
        "laminas-api-tools/api-tools-rpc": "~1.0",
        "laminas-api-tools/api-tools-versioning": "~1.0",
        "laminas/laminas-db": ">=2.3,<2.5",
        "laminas/laminas-eventmanager": ">=2.3,<2.5",
        "laminas/laminas-mvc": ">=2.3,<2.5",
        "laminas/laminas-paginator": ">=2.3,<2.5",
        "laminas/laminas-servicemanager": ">=2.3,<2.5",
        "laminas/laminas-stdlib": ">=2.3,<2.5",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "rwoverdijk/assetmanager": "~1.3"
    },
    "require-dev": {
        "laminas/laminas-http": ">=2.3,<2.5",
        "laminas/laminas-loader": ">=2.3,<2.5",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-apigility": "self.version"
    }
}
